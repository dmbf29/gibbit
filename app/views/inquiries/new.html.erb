<div class="container">
  <div class="row">
    <h1>Make a Request</h1>
    <%= simple_form_for [@inquiry] do |f| %>
      <%= f.input :first_name %>
      <%= f.input :email %>
      <%= f.input :description, as: :text %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
